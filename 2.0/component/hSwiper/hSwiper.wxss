/*滑动样式*/
.hSwiper{
	height: 856rpx;
	overflow: hidden;
	position: relative;
}

.hSwiperCon{
	height: 880rpx;
	will-change: transform;
	position: absolute;
  padding-top:12rpx;
  padding-bottom:12rpx;
  box-sizing: border-box;
}
.item{
	width: 100%;
	height: 100%;
	position:relative;
	float: left;
	box-sizing:border-box;
}